<Window x:Class="LoveBees.View.RegistView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RegistView" Height="350" Width="525">
    <Grid Margin="0,0,103,78">
        <Label x:Name="User" Content="User:" HorizontalAlignment="Left" Margin="104,67,0,0" VerticalAlignment="Top" RenderTransformOrigin="-23.6,-1.038"/>
        <Label x:Name="Password" Content="Password:" HorizontalAlignment="Left" Margin="104,110,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.342,-1.192"/>
        <Button x:Name="Ok" Command="{Binding OkCommand}" Content="OK" HorizontalAlignment="Left" Margin="182,186,0,0" VerticalAlignment="Top" Width="48"/>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="256,186,0,0" VerticalAlignment="Top" Width="56"/>
        <PasswordBox x:Name="Password1" HorizontalAlignment="Left" Margin="182,115,0,0" VerticalAlignment="Top" Width="130"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="104,147,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.342,-1.192"/>
        <PasswordBox x:Name="Password2" HorizontalAlignment="Left" Margin="182,147,0,0" VerticalAlignment="Top" Width="130"/>
        <TextBox x:Name="UserName" Text="{Binding UserName}" HorizontalAlignment="Left" Height="23" Margin="182,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130"/>
    </Grid>
</Window>
